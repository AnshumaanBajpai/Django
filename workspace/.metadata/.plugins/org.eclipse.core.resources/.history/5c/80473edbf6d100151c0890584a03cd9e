from django.http import HttpResponse


# Creating the basic Index view
def index(request):
    return HttpResponse("Rango says hey there world!. Read <br/> <a href = '/rango/about'>about</a> us.")

# Creating an about view
def about(request):
    return HttpResponse("Rango says here is the about page. Go to the <br/> <a href = '/rango/'> index </a> page.")
