from django.http import HttpResponse


## Defining a basic index view
def index(request):
    return HttpResponse("Rango says hey there world!. To know about me click <br/> <a href='/rango/about'> here </a>.")

## Defining an about View
def about(request):
    return HttpResponse("Rango says here is the about page. You can go to the <br/> <a href='/rango/'> index </a>.")